{
	"Evaluate Code Block": {
		"prefix": "eval",
		"body": [
			"{-",
			">>> $1",
			"-}"
		],
		"description": "Evaluate a code block"
	},
	"Language Extension": {
		"prefix": "ext",
		"body": "{-# LANGUAGE $1 #-}"
	},
	"Section Comment": {
		"prefix": "secom",
    "body": [ 
      "{- ",
      "     $1",
      "-}"
    ]	}
}
